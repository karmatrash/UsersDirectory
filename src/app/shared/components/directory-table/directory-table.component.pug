table(class='table table-hover')
    thead
        tr
            th ID
            th Username
            th Email
            th Phone
    tbody
        tr(
        *ngFor="let user of data",
        [routerLink]="['/users', user.key]"
        )
            th {{ user.id }}
            th {{ user.fullName }}
            th {{ user.email }}
            th {{ user.phone | phone }}
